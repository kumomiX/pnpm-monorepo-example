{
  "name": "kumomix",
  "version": "1.0.6",
  "packageManager": "pnpm@7.1.0",
  "exports": {
    ".": {
      "types": "./ui-kit/core/dist/index.d.ts",
      "import": "./ui-kit/core/dist/index.mjs",
      "module": "./ui-kit/core/dist/index.esm.js",
      "require": "./ui-kit/core/dist/index.js"
    },
    "./core": {
      "types": "./ui-kit/core/dist/index.d.ts",
      "import": "./ui-kit/core/dist/index.mjs",
      "module": "./ui-kit/core/dist/index.esm.js",
      "require": "./ui-kit/core/dist/index.js"
    },
    "./infinite": {
      "types": "./ui-kit/infinite/dist/index.d.ts",
      "import": "./ui-kit/infinite/dist/index.mjs",
      "module": "./ui-kit/infinite/dist/index.esm.js",
      "require": "./ui-kit/infinite/dist/index.js"
    },
    "./immutable": {
      "types": "./ui-kit/immutable/dist/index.d.ts",
      "import": "./ui-kit/immutable/dist/index.mjs",
      "module": "./ui-kit/immutable/dist/index.esm.js",
      "require": "./ui-kit/immutable/dist/index.js"
    }
  },
  "files": [
    "ui-kit/core/dist/**/*.{js,d.ts,mjs}",
    "ui-kit/infinite/dist/**/*.{js,d.ts,mjs}",
    "ui-kit/immutable/dist/**/*.{js,d.ts,mjs}",
    "ui-kit/core/package.json",
    "ui-kit/infinite/package.json",
    "ui-kit/immutable/package.json"
  ],
  "license": "MIT",
  "scripts": {
    "csb:install": "pnpm i -g pnpm@7",
    "csb:build": "pnpm install && pnpm build",
    "clean": "pnpm -r run clean",
    "watch": "pnpm -r run watch",
    "build": "pnpm -r run build",
    "types:check": "pnpm -r run types:check",
    "prepublishOnly": "pnpm clean && pnpm build",
    "publish-beta": "pnpm publish --tag beta",
    "format": "prettier --write ./**/*.{ts,tsx}",
    "lint": "eslint . --ext .ts,.tsx --cache",
    "lint:fix": "pnpm lint --fix"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "bunchee": "2.1.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest-dom": "4.0.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-testing-library": "5.5.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "tslib": "2.4.0",
    "typescript": "4.8.2"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "engines": {
    "pnpm": "7"
  }
}
